<?xml version="1.0" encoding="UTF-8"?>
<ip_inst>
    <header>
        <vendor>Pango</vendor>
        <name>Logos Simple Multiplier</name>
        <version>1.1</version>
        <instance>simple_multiplier</instance>
        <family>Logos</family>
        <device>PGL22G</device>
        <package>MBG324</package>
        <speedgrade>-6</speedgrade>
        <generator version="2020.3" build="62942">IP Compiler</generator>
    </header>
    <param_list>
        <param>
            <name>BSIZE</name>
            <value>16</value>
        </param>
        <param>
            <name>OPTIMAL_TIMING</name>
            <value>0</value>
        </param>
        <param>
            <name>PIPEREG_EN_1</name>
            <value>1</value>
        </param>
        <param>
            <name>B_SIGNED</name>
            <value>0</value>
        </param>
        <param>
            <name>OPTIMAL_TIMING_BOOL</name>
            <value>false</value>
        </param>
        <param>
            <name>ASIZE</name>
            <value>16</value>
        </param>
        <param>
            <name>OUTREG_EN</name>
            <value>0</value>
        </param>
        <param>
            <name>INREG_EN</name>
            <value>0</value>
        </param>
        <param>
            <name>PIPEREG_EN_2</name>
            <value>1</value>
        </param>
        <param>
            <name>A_SIGNED</name>
            <value>0</value>
        </param>
        <param>
            <name>PIPE_STATUS</name>
            <value>3</value>
        </param>
        <param>
            <name>ASYNC_RST_BOOL</name>
            <value>false</value>
        </param>
        <param>
            <name>PIPEREG_EN_3</name>
            <value>1</value>
        </param>
        <param>
            <name>ASYNC_RST</name>
            <value>0</value>
        </param>
    </param_list>
    <pin_list>
        <pin>
            <name>a</name>
            <text>a</text>
            <dir>input</dir>
            <pos>left</pos>
            <msb>15</msb>
            <lsb>0</lsb>
        </pin>
        <pin>
            <name>b</name>
            <text>b</text>
            <dir>input</dir>
            <pos>left</pos>
            <msb>15</msb>
            <lsb>0</lsb>
        </pin>
        <pin>
            <name>clk</name>
            <text>clk</text>
            <dir>input</dir>
            <pos>left</pos>
        </pin>
        <pin>
            <name>rst</name>
            <text>rst</text>
            <dir>input</dir>
            <pos>left</pos>
        </pin>
        <pin>
            <name>ce</name>
            <text>ce</text>
            <dir>input</dir>
            <pos>left</pos>
        </pin>
        <pin>
            <name>p</name>
            <text>p</text>
            <dir>output</dir>
            <pos>right</pos>
            <msb>31</msb>
            <lsb>0</lsb>
        </pin>
    </pin_list>
    <synthesis>
        <script><![CDATA[set_option -vlog_std v2001]]></script>
        <script><![CDATA[set_option -disable_io_insertion 1]]></script>
    </synthesis>
    <file_list>
        <output>
            <file pathname="generate.log" format="log" description="Generate Log"/>
            <file pathname="simple_multiplier_tmpl.v" format="verilog" description="Instantiation Template"/>
            <file pathname="simple_multiplier_tmpl.vhdl" format="vhdl" description="Instantiation Template"/>
        </output>
        <source>
            <file pathname="rtl/ipml_mult_v1_1_simple_multiplier.v"/>
            <file pathname="simple_multiplier.v"/>
        </source>
    </file_list>
</ip_inst>
